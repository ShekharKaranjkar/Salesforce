<aura:component >
    <aura:attribute name="issue" type="Object"/>
    <aura:registerEvent name="Selectedissuewith" type="c:IssuewithSearchBarEvent"/>
    <li role="presentation" class="slds-listbox__item" onclick="{!c.setselectedissuewith}">
        <div id="option1" class="slds-media slds-listbox__option slds-listbox__option_plain slds-media_small" role="option">
            <span class="slds-media__figure slds-listbox__option-icon"></span>
            <span class="slds-media__body">
                <span class="slds-truncate" title="{!v.issue.issuewith}"> {!v.issue.issuewith}</span>
            </span>
        </div>
    </li>
</aura:component>